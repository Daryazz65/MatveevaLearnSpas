<Page x:Class="MatveevaLearnSpas.View.Pages.AdditionalMaterialPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MatveevaLearnSpas.View.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="480"
      Title="AdditionalMaterialPage">

        <Grid Grid.Column="1" Grid.RowSpan="2">
            <Grid.RowDefinitions>
                <RowDefinition Height="1*" />
                <RowDefinition Height="1*" />
            </Grid.RowDefinitions>
            <Button Margin="10 0 10 0"
                Name="BylvarBtn" Click="BylvarBtn_Click">
                <TextBlock Text="Бульварное кольцо"/>
            </Button>
            <Button Margin="10 0 10 0" Grid.Row="3"
                Name="MkadBtn" Click="MkadBtn_Click">
                <TextBlock Text="МКАД"/>
            </Button>
        </Grid>
</Page>
