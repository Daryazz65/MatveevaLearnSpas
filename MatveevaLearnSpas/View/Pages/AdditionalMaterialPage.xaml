<Page x:Class="MatveevaLearnSpas.View.Pages.AdditionalMaterialPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MatveevaLearnSpas.View.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="480"
      Title="Дополнительный материал">

        <Grid Grid.Column="1" Grid.RowSpan="2">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*" />
            <ColumnDefinition Width="1*" />
        </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="50" />
                <RowDefinition Height="1*" />
            </Grid.RowDefinitions>
            <Button Margin="5" 
                Name="BylvarBtn" Click="BylvarBtn_Click">
                <TextBlock Text="Бульварное кольцо"/>
            </Button>
            <Button Margin="5" Grid.Column="1"
                Name="MkadBtn" Click="MkadBtn_Click">
                <TextBlock Text="МКАД"/>
            </Button>
        <Frame Name="AdditionalFrame" Grid.ColumnSpan="2" Grid.Row="1" NavigationUIVisibility="Hidden"/>

    </Grid>
</Page>
